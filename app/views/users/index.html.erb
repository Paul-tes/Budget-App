<h1 class="text-center mt-4 text-4xl font-bold shadow decoration-sky-500">Users</h1>

<div id="users" class="">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path, class: "text-white custom-blue-bg btn"%>
